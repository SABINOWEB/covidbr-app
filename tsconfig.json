{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "components/*": [
        "./src/components/*"
      ],
      "css/*": [
        "./src/css/*"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    "src"
  ],
}
